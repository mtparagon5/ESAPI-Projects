                    <StackPanel x:Name="ToolBox_SP" 
                                Orientation="Horizontal"
                                Visibility="Collapsed">
                        <GroupBox x:Name="CICalc_GB" 
                                  Header="CI / R50 Calc" 
                                  Height="auto" 
                                  Width="575"
                                  FontWeight="Bold"
                                  VerticalAlignment="Center"
                                  Visibility="Hidden">

                            <StackPanel Orientation="Vertical"
                                            Width="560">
                                <StackPanel Orientation="Horizontal"
                                                Margin="10,0,0,5" >
                                    <TextBlock x:Name="RatioTypeCB_TB" 
                                                    Text="Type:"
                                                    Width="85"
                                                    Padding="1"
                                                    VerticalAlignment="Center"
                                                    TextAlignment="left"
                                                    FontWeight="Bold" />
                                    <ComboBox x:Name="RatioType_ComboBox"
                                                    Margin="1" 
                                                    Width="125"
                                                    FontWeight="Regular"
                                                    SelectionChanged="RatioType_SelectionChanged"/>
                                    <TextBlock x:Name="RatioCalcLimitLabel_TB"
                                                    Margin="140,0,0,0"
                                                    Text="Limit/Range:"
                                                    VerticalAlignment="Center"
                                                    Padding="1"
                                                    Width="85"
                                                    FontWeight="Bold"/>
                                    <TextBlock x:Name="ConformityRangeLimit_TB"
                                                    Text=""
                                                    Margin="3,0,0,0"
                                                    VerticalAlignment="Center"
                                                    Width="105"
                                                    FontWeight="Regular"
                                                    Padding="1"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal"
                                                Width="auto"
                                                Margin="10,0,0,5" 
                                                VerticalAlignment="Center">

                                    <TextBlock x:Name="RatioStructureCB_TB" 
                                                    Text="Structure:"
                                                    Width="85"
                                                    Padding="1"
                                                    VerticalAlignment="Center"
                                                    TextAlignment="left"
                                                    FontWeight="Bold" />
                                    <ComboBox x:Name="RatioStructure_ComboBox"
                                                    Margin="1" 
                                                    Width="125"
                                                    FontWeight="Regular"/>
                                    <TextBlock x:Name="RatioCalcResultLabel_TB"
                                                    Margin="140,0,0,0"
                                                    Width="85"
                                                    Text="Ratio:"
                                                    VerticalAlignment="Center"
                                                    Padding="1"
                                                    FontWeight="Bold"/>
                                    <TextBlock x:Name="RatioCalcResult_TB"
                                                    Text=""
                                                    Width="105"
                                                    Margin="3,0,0,0"
                                                    VerticalAlignment="Center"
                                                    FontWeight="Regular"
                                                    Padding="1"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal"
                                                Width="auto"
                                                Margin="10,0,0,5" 
                                                VerticalAlignment="Center">
                                    <TextBlock x:Name="TargetCB_TB" 
                                                    Text="Target:"
                                                    Width="85"
                                                    Padding="1"
                                                    VerticalAlignment="Center"
                                                    TextAlignment="left"
                                                    FontWeight="Bold" />
                                    <ComboBox x:Name="RatioTarget_ComboBox"
                                                    Margin="1"
                                                    Width="125"
                                                    FontWeight="Regular"/>
                                    <TextBlock x:Name="RxDoseLabel_TB" 
                                                    Text="Rx Dose" 
                                                    Padding="1"
                                                    Margin="10,0,0,0"
                                                    TextAlignment="Left" 
                                                    HorizontalAlignment="Center" 
                                                    VerticalAlignment="Center" 
                                                    Width="auto" 
                                                    FontWeight="Bold"/>
                                    <TextBox x:Name="RxDose_Input" 
                                                    Background="LightBlue" 
                                                    Margin="3,0,0,0" 
                                                    Padding="1"
                                                    Height="18" 
                                                    HorizontalAlignment="Center" 
                                                    HorizontalContentAlignment="Center" 
                                                    Width="50" 
                                                    FontWeight="Normal"/>
                                    <Button Name="CalculateConformity_Btn" 
                                                Content="Calculate Conformity" 
                                                Width="130"
                                                Margin="90,0,0,0"
                                                FontWeight="Regular"
                                                Click="CalculateConformity_Clicked" 
                                                Background="#DCDCDC"
                                                HorizontalAlignment="Right"/>
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox x:Name="CustomConstraint_GB" 
                                  Header="Add Custom Constraint" 
                                  Height="auto" 
                                  Width="575"
                                  FontWeight="Bold"
                                  Visibility="Hidden">
                            <StackPanel Orientation="Vertical">
                                <StackPanel Orientation="Horizontal"
                                            Margin="0,0,0,5">
                                    <TextBlock x:Name="Type_TB" 
                                               HorizontalAlignment="Left"
                                               Margin="10,1"
                                               TextWrapping="Wrap" 
                                               Text="Type:"
                                               VerticalAlignment="Center" 
                                               Width="85"
                                               FontWeight="Bold"/>
                                    <ComboBox x:Name="ConstraintType_ComboBox" 
                                               HorizontalAlignment="Left"
                                               Margin="1" 
                                               Text="" 
                                               FontWeight="Regular"
                                               VerticalAlignment="Center" 
                                               SelectionChanged="ConstraintTypeSelected"
                                               MinWidth="125"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal"
                                            Margin="0,0,0,5">
                                    <TextBlock x:Name="Structure_TB" 
                                               HorizontalAlignment="Left"
                                               Margin="10,1"
                                               TextWrapping="Wrap" 
                                               Text="Structure:"
                                               VerticalAlignment="Center" 
                                               Width="85"
                                               FontWeight="Bold"/>
                                    <ComboBox x:Name="ConstraintStructure_ComboBox" 
                                               HorizontalAlignment="Left"
                                               Margin="1" 
                                               FontWeight="Regular"
                                               VerticalAlignment="Center" 
                                               MinWidth="125"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal"
                                            Margin="0,0,0,0">
                                    <StackPanel x:Name="DoseLimit_SP"
                                                Orientation="Horizontal">
                                        <TextBlock x:Name="DoseLimit_TB" 
                                                   HorizontalAlignment="Left"
                                                   Margin="10,1"
                                                   TextWrapping="Wrap" 
                                                   Text="Dose Limit:"
                                                   VerticalAlignment="Center" 
                                                   Width="83"
                                                   FontWeight="Bold"/>
                                        <TextBox x:Name="DoseLimit_Input" 
                                                 Background="LightBlue" 
                                                 Margin="3,0,0,0" 
                                                 Padding="1"
                                                 Height="18"
                                                 HorizontalAlignment="Center" 
                                                 HorizontalContentAlignment="Center"
                                                 Width="45" 
                                                 FontWeight="Normal"/>
                                        <TextBlock x:Name="DoseLimitLabel_TB" 
                                                   Margin="0" 
                                                   Padding="1"
                                                   Height="18"
                                                   HorizontalAlignment="Left" 
                                                   VerticalAlignment="Bottom"
                                                   Width="30" 
                                                   FontWeight="Normal"
                                                   Text="Gy"/>
                                    </StackPanel>
                                    <StackPanel x:Name="VolumeLimit_SP"
                                                Orientation="Horizontal">
                                        <TextBlock x:Name="VolumeLimit_TB" 
                                                   HorizontalAlignment="Left"
                                                   Margin="10,0,10,1"
                                                   TextWrapping="Wrap" 
                                                   Text="Volume Limit:"
                                                   VerticalAlignment="Center" 
                                                   Width="83"
                                                   FontWeight="Bold"/>
                                        <TextBox x:Name="VolumeLimit_Input" 
                                                 Background="LightBlue" 
                                                 Margin="3,0,0,0" 
                                                 Padding="1"
                                                 Height="18"
                                                 HorizontalAlignment="Center" 
                                                 HorizontalContentAlignment="Center"
                                                 Width="45" 
                                                 FontWeight="Normal"/>
                                        <TextBlock x:Name="VolumeLimitLabel_TB" 
                                                   Margin="0" 
                                                   Padding="1"
                                                   Height="18"
                                                   HorizontalAlignment="Left" 
                                                   VerticalAlignment="Bottom"
                                                   Width="30" 
                                                   FontWeight="Normal"/>
                                    </StackPanel>
                                    <Button Name="ClearConstraintLimits_Btn" 
                                            Content="Clear Limits" 
                                            Width="90"
                                            Margin="10,0,5,0"
                                            FontWeight="Regular"
                                            Click="ClearConstraintLimits_Clicked" 
                                            Background="#DCDCDC"/>
                                    <Button Name="AddConstraint_Btn" 
                                            Content="Add Constraint" 
                                            Width="90"
                                            Margin="0"
                                            FontWeight="Regular"
                                            Click="AddConstraint_Clicked" 
                                            Background="#DCDCDC"/>
                                </StackPanel>

                            </StackPanel>
                        </GroupBox>
                    </StackPanel>
                    <StackPanel x:Name="NotesAndConformityDG_SP" 
                                Orientation="Horizontal"
                                HorizontalAlignment="Left"
                                Visibility="Collapsed">
                        <GroupBox x:Name="Notes_GB" 
                                    Header="Notes" 
                                    Visibility="Hidden"
                                    VerticalAlignment="Top"
                                    Height="auto" 
                                    Width="575"
                                    FontWeight="Bold">
                            <StackPanel Orientation="Horizontal">
                                <TextBox x:Name="Note_TB" 
                                            Width="514"
                                            Padding="1"
                                            HorizontalAlignment="Left"
                                            FontWeight="Regular"
                                            TextWrapping="Wrap"
                                            AcceptsReturn="True">
                                </TextBox>
                                <Button Name="ClearNote_Btn" 
                                    Content="Clear" 
                                    Width="49"
                                    FontWeight="Regular"
                                    VerticalAlignment="Top"
                                    Click="ClearNote_Clicked" 
                                    Background="#DCDCDC"/>
                            </StackPanel>
                        </GroupBox>
                        <GroupBox x:Name="Ratio_GB" 
                                  Header="CI / R50 Statistics"
                                  Height="auto"
                                  Width="575"
                                  FontWeight="Bold"
                                  Visibility="Hidden">
                            <DataGrid Name="Ratio_DG" 
                                      HorizontalContentAlignment="Center" 
                                      Margin="0,10,-2,-2"
                                      FontWeight="Normal"
                                      Background="Transparent">
                                <DataGrid.Resources>
                                    <Style TargetType="{x:Type DataGridColumnHeader}">
                                        <Setter Property="Background" 
                                                    Value="LightBlue"/>
                                        <Setter Property="FontWeight" 
                                                    Value="Bold"/>
                                        <Setter Property="BorderThickness" 
                                                    Value="0,0,1,2"/>
                                        <Setter Property="BorderBrush" 
                                                    Value="Black"/>
                                        <Setter Property="HorizontalContentAlignment" 
                                                    Value="Center"/>
                                    </Style>
                                </DataGrid.Resources>
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Target Id"
                                                            Binding="{Binding targetId}" 
                                                            Width=".95*" 
                                                            IsReadOnly="True">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="HorizontalAlignment" 
                                                            Value="Center" />
                                                <Setter Property="VerticalAlignment" 
                                                            Value="Center" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Type" 
                                                            Binding="{Binding ratioType}"
                                                            Width=".8*" 
                                                            IsReadOnly="True">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="HorizontalAlignment"
                                                            Value="Center" />
                                                <Setter Property="VerticalAlignment" 
                                                            Value="Center" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Limit"
                                                            Binding="{Binding rangeLimit}" 
                                                            Width="*" 
                                                            IsReadOnly="True">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="HorizontalAlignment" 
                                                            Value="Center" />
                                                <Setter Property="VerticalAlignment" 
                                                            Value="Center" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Actual"
                                                            Binding="{Binding conformity}" 
                                                            Width=".8*" 
                                                            IsReadOnly="True">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="HorizontalAlignment" 
                                                            Value="Center" />
                                                <Setter Property="VerticalAlignment" 
                                                            Value="Center" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTextColumn Header="Result"
                                                            Binding="{Binding result}" 
                                                            Width=".8*" 
                                                            IsReadOnly="True">
                                        <DataGridTextColumn.ElementStyle>
                                            <Style TargetType="TextBlock">
                                                <Style.Triggers>
                                                    <Trigger Property="Text" 
                                                             Value="MET">
                                                        <Setter Property="Background"
                                                                Value="LightGreen"/>
                                                    </Trigger>
                                                    <Trigger Property="Text" 
                                                             Value="Acceptable">
                                                        <Setter Property="Background" 
                                                                Value="#f4b642"/>
                                                    </Trigger>
                                                    <Trigger Property="Text" 
                                                             Value="NOT MET">
                                                        <Setter Property="Background" 
                                                                Value="#F63C3C"/>
                                                    </Trigger>
                                                </Style.Triggers>
                                                <Setter Property="HorizontalAlignment" 
                                                            Value="Center" />
                                                <Setter Property="VerticalAlignment" 
                                                            Value="Center" />
                                            </Style>
                                        </DataGridTextColumn.ElementStyle>
                                    </DataGridTextColumn>
                                    <DataGridTemplateColumn Header="" 
                                                                Width=".3*">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>
                                                <Button Name="ConformityItemRemove_Btn" 
                                                            Content="n/a" 
                                                            Click="ConformityItemRemoveButton_Clicked" 
                                                            Background="#DCDCDC"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                </DataGrid.Columns>
                            </DataGrid>
                        </GroupBox>
                    
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" 
                                Margin="0,0,0,0">
                        <GroupBox x:Name="TargetStats_GB" 
                                  Header="Target Dose Statistics" 
                                  Height="auto" 
                                  Width="575"
                                  FontWeight="Bold">
                            <DataGrid Name="TargetCoverage_DG" 
                                      HorizontalContentAlignment="Center" 
