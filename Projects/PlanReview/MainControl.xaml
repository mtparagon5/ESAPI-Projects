<UserControl x:Class="PlanReview.MainControl"
             Name="MainUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             Height="auto" 
             Width="1200" 
             Background="#FEFEFE"
             FontFamily="Arial">

    <Border Name="MainBorder" 
            CornerRadius="2" 
            BorderBrush="LightGray" 
            BorderThickness="1">
        <Viewbox>
            <Grid Name="MainGrid"
                  Margin="20">
                <StackPanel Orientation="Vertical">
                    <StackPanel Margin="0,0,0,10">
                        <StackPanel Orientation="Horizontal"
                                    HorizontalAlignment="Right"
                                    Margin="0,10,10,0">
                            <Button x:Name="AddNote_Btn" 
                                    Click="AddNote_Clicked" 
                                    Content="Add Note" 
                                    FontWeight="Regular" 
                                    Margin="0,0,5,0" 
                                    Height="auto"
                                    Width="100"/>
                            <Button x:Name="PrintVisual_Btn" 
                                    Click="Print_Visual" 
                                    Content="Print" 
                                    FontWeight="Regular" 
                                    Margin="0" 
                                    Height="auto"
                                    Width="100"/>
                        </StackPanel>
                        <StackPanel Orientation="Vertical"
                                    HorizontalAlignment="Right"
                                    Margin="0,5,10,10">
                            <Button x:Name="AddConformityToolBox_Btn" 
                                    Click="AddConformityToolBox_Clicked" 
                                    Content="Add Conformity ToolBox" 
                                    FontWeight="Regular" 
                                    Margin="0,0,0,5" 
                                    Height="auto"
                                    Width="205"/>
                            <Button x:Name="AddCustomConstraint_Btn" 
                                    Click="AddCustomConstraint_Clicked" 
                                    Content="Add Custom Constraint ToolBox" 
                                    FontWeight="Regular" 
                                    Margin="0,0,0,0" 
                                    Height="auto"
                                    Width="205"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal"
                                    HorizontalAlignment="Center"
                                    Margin="0,20">
                            <TextBlock x:Name="Header_TB"
                                    HorizontalAlignment="Center"
                                    Text="Plan Review" 
                                    Width="auto"
                                    FontWeight="Bold"
                                    FontSize="16"/>
                        </StackPanel>
                
                        <StackPanel Orientation="Horizontal" 
                                    HorizontalAlignment="Left">
                            <TextBlock x:Name="PatientId_TB" 
                                       HorizontalAlignment="Left" 
                                       Margin="10,1" 
                                       TextWrapping="Wrap" 
                                       Text="Patient Id:" 
                                       Width="85" 
                                       FontWeight="Bold"/>
                            <TextBlock x:Name="PatientId" 
                                       HorizontalAlignment="Left" 
                                       Margin="1"
                                       TextWrapping="Wrap"
                                       Text="" 
                                       MinWidth="100"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" 
                                    HorizontalAlignment="Left">
                            <TextBlock x:Name="PatientName_TB" 
                                       HorizontalAlignment="Left"
                                       Margin="10,1"
                                       TextWrapping="Wrap" 
                                       Text="Patient Name:"
                                       Width="85"
                                       FontWeight="Bold"/>
                            <TextBlock x:Name="PatientName" 
                                       HorizontalAlignment="Left"
                                       Margin="1" 
                                       TextWrapping="Wrap" 
                                       Text="" 
                                       MinWidth="100"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" 
                                    HorizontalAlignment="Left" Width="auto">
                            <TextBlock x:Name="PrimaryOnc_TB" 
                                       HorizontalAlignment="Left" 
                                       Margin="10,1" 
                                       TextWrapping="Wrap"
                                       Text="Primary Onc:"
                                       Width="85"
                                       FontWeight="Bold"/>
                            <TextBlock x:Name="PrimaryOnc" 
                                       HorizontalAlignment="Left" 
                                       Margin="1" 
                                       TextWrapping="Wrap"
                                       Text="" 
                                       MinWidth="100"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" 
                                    HorizontalAlignment="Left"
                                    Width="auto">
                            <TextBlock x:Name="PlanId_TB" 
                                       HorizontalAlignment="Left" 
                                       Margin="10,1" 
                                       TextWrapping="Wrap" 
                                       Text="Plan Id:"
                                       Width="85"
                                       FontWeight="Bold"/>
                            <TextBlock x:Name="PlanId"
                                       HorizontalAlignment="Left" 
                                       Margin="1"
                                       TextWrapping="Wrap" 
                                       Text="" 
                                       MinWidth="100"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" 
                                    HorizontalAlignment="Left"
                                    Width="auto">
                            <TextBlock x:Name="PlanStatus_TB" 
                                       HorizontalAlignment="Left" 
                                       Margin="10,1" 
                                       TextWrapping="Wrap" 
                                       Text="Plan Status:"
                                       Width="85"
                                       FontWeight="Bold"/>
                            <TextBlock x:Name="PlanStatus"
                                       HorizontalAlignment="Left" 
                                       Margin="1"
                                       TextWrapping="Wrap" 
                                       Text="" 
                                       MinWidth="100"/>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel x:Name="ManualCalc_SP" 
                                Orientation="Horizontal"
                                VerticalAlignment="Center">
                        <GroupBox Header="Manually Calculate DVH Stats" 
                                  Height="auto" 
                                  Width="1150"
                                  FontWeight="Bold"
                                  VerticalAlignment="Center"
                                  Margin="0,0,0,0">
                            <StackPanel x:Name="VolumeAtDose_WP" 
                                        Orientation="Horizontal"
                                       VerticalAlignment="Center"
                                        Margin="0,10,0,0">
                                <StackPanel Orientation="Horizontal"
                                            Width="575">
                                    <TextBlock x:Name="V_TB" 
                                               Text="V" 
                                               Height="15" 
                                               TextAlignment="Right" 
                                               HorizontalAlignment="Center" 
                                               VerticalAlignment="Center" 
                                               Width="13" 
                                               FontWeight="Normal"/>
                                    <TextBox x:Name="volumeAtDose_Input" 
                                             Background="LightBlue" 
                                             Margin="3,0,0,0" 
                                             Height="18" 
                                             HorizontalAlignment="Center" 
                                             HorizontalContentAlignment="Center" 
                                             Width="35" 
                                             TextChanged="volumeAtDose_Input_Changed" 
                                             FontWeight="Normal"/>
                                    <TextBlock x:Name="volumeAtDoseResult_TB" 
                                               Text="volume at dose" 
                                               Margin="2,0,0,0" 
                                               Width="150"
                                               Padding="1"
                                               VerticalAlignment="Center" 
                                               FontWeight="Normal"/>
                                    <TextBlock x:Name="D_TB" 
                                               Text="D" 
                                               Height="15" 
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                               Margin="0,0,0,0" 
                                               FontWeight="Normal"/>
                                    <TextBox x:Name="doseAtVolume_Input" 
                                             Background="LightBlue" 
                                             Margin="3,0,0,0" 
                                             Height="18"
                                             HorizontalAlignment="Center" 
                                             HorizontalContentAlignment="Center"
                                             Width="35" 
                                             TextChanged="doseAtVolume_Input_Changed" 
                                             FontWeight="Normal"/>
                                    <TextBlock x:Name="doseAtVolumeResult_TB"
                                               Text="dose at volume"
                                               Margin="2,0,0,0"
                                               Width="150"
                                               Padding="1"
                                               HorizontalAlignment="Center" 
                                               VerticalAlignment="Center" 
                                               FontWeight="Normal" />
                                </StackPanel>
                                <StackPanel Orientation="Horizontal"
                                            Width="560">
                                    <StackPanel Orientation="Horizontal"
                                                Width="320"
                                                VerticalAlignment="Center">
                                        <TextBlock x:Name="StructureVolume_TB" 
                                                   Text="Select a structure to view proximity statistics"
                                                   Width="320"
                                                   Padding="1"
                                                   VerticalAlignment="Center"
                                                   TextAlignment="Right"
                                                   FontWeight="Normal" />
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <ComboBox x:Name="Structure_ComboBox"
                                                  Margin="20,0,0,0" 
                                                  VerticalAlignment="Top" 
                                                  Width="125"
                                                  FontWeight="Regular"
                                                  SelectionChanged="Structure_ComboBox_SelectionChanged"/>
                                        <CheckBox x:Name="absDose_CB" 
                                              Content="Gy&#x9;" 
                                              Margin="10,0,0,0"
                                              VerticalAlignment="Center" 
                                              RenderTransformOrigin="0.429,0.422"
                                              Width="38" 
                                              FontWeight="Normal" 
                                              IsChecked="True" 
                                              Checked="absDose_CB_Checked" 
                                              Unchecked="absDose_CB_UnChecked"/>
                                        <CheckBox x:Name="absVolume_CB"
                                              Content="cc"
                                              Margin="10,0,0,0" 
                                              VerticalAlignment="Center"
                                              IsChecked="True" 
                                              Checked="absVolume_CB_Checked" 
                                              Width="32" 
                                              FontWeight="Normal" 
                                              Unchecked="absVolume_CB_UnChecked"/>
                                    </StackPanel>
                                
                                
                                </StackPanel>
                            </StackPanel>
                        </GroupBox>
                    </StackPanel>
